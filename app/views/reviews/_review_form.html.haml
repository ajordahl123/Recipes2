%div=f.label :stars, "Rating"
=f.number_field :stars, required: true, min: 0, max: 5

%div=f.label :text, "Review text"
=f.text_field :text

%div= f.label :image, "Photo"
=f.file_field :image

=f.hidden_field :user_id, value: current_user.id

=f.hidden_field :recipe_id, value: @recipe.id
